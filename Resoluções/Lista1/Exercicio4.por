programa {
  funcao inicio() {
    real distancia, velocidade, tempo

    escreva("informe o velocidade em que percorre: ")
    leia(velocidade)

    escreva("informe no formato de horas quanto tempo percorreu: ")
    leia(tempo)

    distancia = velocidade*tempo

    escreva("A distancia percorrida foi de => ", distancia)

  }
}
